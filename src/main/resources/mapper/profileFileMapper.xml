<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.app.ggumteo.mapper.file.ProfileFileMapper">

    <insert id="insertPostFile" useGeneratedKeys="true" keyProperty="id" keyColumn="id">
